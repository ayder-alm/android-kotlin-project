<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ayder.typicalapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".TheApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TypicalApp">
        <activity android:name=".ui.base.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.base.NasaDailyPicturesActivity"/>
        <activity android:name=".ui.base.NasaDailyPictureActivity"/>
        <activity android:name=".ui.base.UsersActivity"/>
        <activity android:name=".ui.base.UserActivity"/>
    </application>

</manifest>